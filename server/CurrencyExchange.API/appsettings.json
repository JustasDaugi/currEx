{
  "CurrencyApi": {
    "ConversionEndpoint": "https://v6.exchangerate-api.com/v6/API_KEY/pair",
    "HistoricalEndpoint": "https://v6.exchangerate-api.com/v6/API_KEY/history"
  },
  "Mongo": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "CurrencyExchangeDb"
  },
  "JwtSettings": {
    "Key": "MySuperSecretKey123!MySuperSecretKey123!",
    "Issuer": "CurrencyExchange.API",
    "Audience": "CurrencyExchange.Client",
    "ExpiresInMinutes": "15"
  },
  "GoogleAuth": {
    "ClientId": "your-client-id.apps.googleusercontent.com",
    "ClientSecret": "your-client-secret",
    "RedirectUri": "http://localhost:5243/api/auth/google/callback"
  },
  "ClientApp": {
    "BaseUrl": "http://localhost:3000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
